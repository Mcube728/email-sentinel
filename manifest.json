{
    "name": "email_sentinel",
    "description": "Detects any phishing links in your emails.",
    "version": "0.1",
    "manifest_version": 3,

    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },

    "action": {
        "default_popup": "html/popup.html"
    },

    "permissions": ["activeTab", "scripting"]
}
